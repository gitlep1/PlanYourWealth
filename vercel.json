{
  "builds": [
    {
      "src": "/back-end/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "/front-end/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/back-end/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/front-end/dist/index.html"
    }
  ]
}
